{
  "name": "mckltech/zoho-crm-php",
  "description": "Zoho CRM API client for PHP",
  "keywords": [
    "api",
    "guzzle",
    "zoho"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Highways.io",
      "homepage": "https://www.highways.io"
    }
  ],
  "support": {
    "issues": "https://github.com/MCKLtech/zoho-crm-php/issues",
    "source": "https://github.com/MCKLtech/zoho-crm-php"
  },
  "autoload": {
    "psr-4": {
      "Zoho\\": [
        "src"
      ]
    }
  },
  "require": {
      "php": "^7.4|^8.0",
      "ext-json": "*",
      "php-http/client-common": "^1.9 || ^2.0",
      "php-http/client-implementation": "^1.0",
      "php-http/discovery": "^1.4",
      "php-http/httplug": "^1.0 || ^2.0",
      "php-http/message": "^1.7",
      "psr/http-message": "^1.0"
  },
  "require-dev": {
      "guzzlehttp/psr7": "^1.7.0",
      "php-http/mock-client": "^1.4",
      "phpunit/phpunit": "^7.5 || ^8.5 || ^9.3"
  },
  "config": {
    "sort-packages": true
  }
}
